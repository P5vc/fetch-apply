# The current upgrade iteration of Fetch Apply:
fa_var_UPGRADE_ITERATION=5

# The oldest iteration of Fetch Apply compatible with an automated upgrade:
fa_var_COMPATIBLE_UPGRADE_ITERATION=2

# Manually disable automated upgrades (change to "YES"):
fa_var_DISABLE_AUTOMATED_UPGRADES="No"


# Files to be preserved for this upgrade iteration:
# /etc/fetchapply
# /etc/cron.d/fa
# {fa_var_FA_ROOT}/status/completedInits
# {fa_var_LOG_FILE_PATH}
# {fa_var_LOG_FILE_PATH}.old
